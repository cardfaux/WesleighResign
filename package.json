{
  "name": "expressprismicwebpacktemplate",
  "version": "1.0.0",
  "description": "template for express, prismic, and webpack configuration",
  "main": "index.js",
  "scripts": {
    "backend:build": "node server/server.mjs",
    "backend:dev": "nodemon server/server.mjs",
    "frontend:build": "webpack --progress --config ./webpack/webpack.prod.js",
    "frontend:dev": "webpack serve --progress --config ./webpack/webpack.dev.js",
    "dev": "concurrently --kill-others \"npm run backend:dev\" \"npm run frontend:dev\"",
    "build-report": "webpack-bundle-analyzer --port 4200 ./__dist/stats.json"
  },
  "keywords": [
    "webpack",
    "express",
    "prismic",
    "template"
  ],
  "author": "James Hagood",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/preset-env": "^7.18.6",
    "@squoosh/lib": "^0.4.0",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-webpack-plugin": "^3.2.0",
    "glslify-loader": "^2.0.0",
    "image-minimizer-webpack-plugin": "^3.2.3",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "include-media": "^1.4.10",
    "lil-gui": "^0.16.1",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.18",
    "postcss-loader": "^7.0.0",
    "prettier": "2.7.1",
    "raw-loader": "^4.0.2",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "terser-webpack-plugin": "^5.3.3",
    "three": "^0.142.0",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@prismicio/client": "^6.0.0",
    "@prismicio/helpers": "^2.3.1",
    "body-parser": "^1.20.0",
    "concurrently": "^7.2.2",
    "dotenv": "^16.0.1",
    "errorhandler": "^1.5.1",
    "express": "^4.18.1",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.2.6",
    "pug": "^3.0.2",
    "ua-parser-js": "^1.0.2"
  }
}
